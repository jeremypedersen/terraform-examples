# Required variables
access_key = "your_access_key"
access_key_secret = "your_access_key_secret"
ssh_key_name = "your_ssh_key_name" # This is just the name for the SSH key generated by terraform, no need to supply a real keyfile
password = "your_ecs_login_password" # A password for logging into ECS instances created by terraform
# Optional variables
# region = "" # Region your resources will be deployed in (defaults to Singapore)
# vpc_cidr_block = "" # CIDR block for the VPC that will be deployed (defaults to 192.168.0.0/16)
# dev_vswitch_cidr_block = "" # CIDR block for the dev subnet vSwitch (defaults to 192.168.1.0/24)
# staging_vswitch_cidr_block = "" # CIDR block for the dev subnet vSwitch (defaults to 192.168.2.0/24)
# prod_vswitch_cidr_block = "" # CIDR block for the dev subnet vSwitch (defaults to 192.168.3.0/24)
# management_vswitch_cidr_block = "" # CIDR block for the dev subnet vSwitch (defaults to 192.168.0.0/24)
